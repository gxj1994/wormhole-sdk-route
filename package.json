{
  "name": "@mayanfinance/wormhole-sdk-route",
  "version": "1.26.0",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "",
  "description": "This package implements an AutomaticRoute for use with the Wormhole Typescript SDK",
  "files": [
    "dist"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "docs": "typedoc",
    "example:swap": "npx tsx examples/swap.ts"
  },
  "dependencies": {
    "@mayanfinance/swap-sdk": "11.0.0",
    "axios": "^1.11.0"
  },
  "peerDependencies": {
    "@mysten/sui": "^1.37.3",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "@wormhole-foundation/sdk-base": "^3.4.0",
    "@wormhole-foundation/sdk-connect": "^3.4.0",
    "@wormhole-foundation/sdk-evm": "^3.4.0",
    "@wormhole-foundation/sdk-solana": "^3.4.0",
    "@wormhole-foundation/sdk-sui": "^3.4.0",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "dotenv": "^16.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
